{
  "name": "selfsea-mobile",
  "version": "1.0.1",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "android-release": "react-native run-android --variant=release",
    "ios": "react-native run-ios",
    "start": "react-native start",
    "develop": "run-p \"codegen --watch\" start",
    "test": "jest",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "lint:fix": "eslint . --ext .js,.jsx,.ts,.tsx --fix",
    "codegen": "graphql-codegen --config codegen.yml",
    "prepare": "husky install",
    "pre-commit": "npm run spell-check:new && npm run lint && npm run test",
    "spell-check:new": "git diff --name-only | cspell --no-must-find-files --no-progress --file-list stdin",
    "build-android-debug": "cd android && ./gradlew clean && ./gradlew assembleDebug && cd .."
  },
  "dependencies": {
    "@apollo/client": "^3.5.7",
    "@invertase/react-native-apple-authentication": "^2.1.5",
    "@react-native-async-storage/async-storage": "^1.15.16",
    "@react-native-community/cookies": "^5.0.1",
    "@react-native-firebase/app": "^14.1.0",
    "@react-native-firebase/auth": "^14.2.1",
    "@react-native-google-signin/google-signin": "^7.0.4",
    "@react-navigation/bottom-tabs": "^6.0.9",
    "@react-navigation/drawer": "^6.1.8",
    "@react-navigation/material-top-tabs": "^6.0.6",
    "@react-navigation/native": "^6.0.6",
    "@react-navigation/stack": "^6.0.11",
    "@types/react-redux": "^7.1.20",
    "base-64": "^1.0.0",
    "graphql": "^15.8.0",
    "graphql-request": "^3.7.0",
    "graphql-tag": "^2.12.6",
    "react": "17.0.2",
    "react-native": "0.67.0",
    "react-native-axios": "^0.17.1",
    "react-native-config": "^1.4.5",
    "react-native-dotenv": "^3.3.1",
    "react-native-gesture-handler": "^2.1.0",
    "react-native-get-random-values": "^1.7.2",
    "react-native-instagram-login": "^2.0.5",
    "react-native-linear-gradient": "^2.5.6",
    "react-native-pager-view": "^5.4.9",
    "react-native-password-strength-meter": "^0.0.5",
    "react-native-reanimated": "^2.3.1",
    "react-native-safe-area-context": "^3.3.2",
    "react-native-screens": "^3.10.1",
    "react-native-select-dropdown": "^1.6.0",
    "react-native-tab-view": "^3.1.1",
    "react-native-webview": "^11.15.0",
    "react-redux": "^7.2.6",
    "redux": "^4.1.2",
    "redux-devtools-extension": "^2.13.9",
    "redux-observable": "^2.0.0",
    "redux-saga": "^1.1.3",
    "rxjs": "^7.4.0",
    "typesafe-actions": "^5.1.0",
    "uuid": "^8.3.2",
    "zxcvbn": "^4.4.2"
  },
  "devDependencies": {
    "@babel/core": "^7.12.9",
    "@babel/plugin-proposal-export-namespace-from": "^7.16.0",
    "@babel/runtime": "^7.12.5",
    "@graphql-codegen/cli": "^2.3.0",
    "@graphql-codegen/introspection": "^2.1.0",
    "@graphql-codegen/typescript": "^2.4.1",
    "@graphql-codegen/typescript-graphql-files-modules": "^2.1.0",
    "@graphql-codegen/typescript-graphql-request": "^4.3.2",
    "@graphql-codegen/typescript-operations": "^2.2.1",
    "@react-native-community/eslint-config": "^2.0.0",
    "@types/jest": "^26.0.23",
    "@types/react-native": "^0.66.4",
    "@types/react-test-renderer": "^17.0.1",
    "babel-jest": "^27.1.3",
    "babel-plugin-module-resolver": "^4.1.0",
    "cspell": "^5.18.5",
    "eslint": "^7.14.0",
    "husky": "^7.0.0",
    "jest": "^27.5.1",
    "metro-react-native-babel-preset": "^0.66.2",
    "npm-run-all": "^4.1.5",
    "react-test-renderer": "17.0.2",
    "ts-jest": "^27.1.3",
    "typescript": "^4.4.4"
  },
  "resolutions": {
    "@types/react": "^17"
  },
  "jest": {
    "preset": "react-native",
    "globals": {
      "ts-jest": {
        "tsconfig": "tsconfig.spec.json"
      }
    },
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ],
    "transform": {
      "^.+\\.(js|jsx)$": "babel-jest",
      "^.+\\.(ts|tsx)$": "ts-jest"
    },
    "cacheDirectory": ".jest/cache"
  }
}
